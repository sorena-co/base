<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Course.
    -->
    <changeSet id="20180812190644-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="course_program"
                                 constraintName="fk_course_program_courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>
        <addForeignKeyConstraint baseColumnNames="programs_id"
                                 baseTableName="course_program"
                                 constraintName="fk_course_program_programs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="program"/>
        
        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="course_person"
                                 constraintName="fk_course_person_courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>
        <addForeignKeyConstraint baseColumnNames="people_id"
                                 baseTableName="course_person"
                                 constraintName="fk_course_person_people_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>
        
    </changeSet>
</databaseChangeLog>
